{
  "name": "react_native_boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android:staging:debug": "ENVFILE=.env.staging react-native run-android",
    "android:production:debug": "ENVFILE=.env.production react-native run-android",
    "android:staging:release": "ENVFILE=.env.staging  react-native run-android --variant=release",
    "android:production:release": "ENVFILE=.env.production react-native run-android --variant=release",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:staging:debug": "ENVFILE=.env.staging react-native run-ios",
    "ios:production:debug": "ENVFILE=.env.production react-native run-ios",
    "setup:ios": "cd ios && pod install && cd ..",
    "clean:ios": "rm -rf ios/Pods && rm -rf ios/Podfile.lock",
    "clean:modules": "rm -rf node_modules && rm -rf package-lock.json",
    "test": "jest",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx ./app --fix",
    "prettier": "prettier --write ./app",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write ./app",
      "eslint --fix ./app"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.5",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-community/netinfo": "^11.1.0",
    "@react-native-firebase/analytics": "^18.6.1",
    "@react-native-firebase/app": "^18.6.1",
    "@react-native-firebase/auth": "^18.6.1",
    "@react-native-firebase/crashlytics": "^18.6.1",
    "@react-native-firebase/database": "^18.6.1",
    "@react-native-firebase/firestore": "^18.6.1",
    "@react-native-firebase/messaging": "^18.6.1",
    "@react-native-firebase/storage": "^18.6.1",
    "@react-native-google-signin/google-signin": "^10.1.1",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "appcenter": "^5.0.0",
    "appcenter-analytics": "^5.0.0",
    "appcenter-crashes": "^5.0.0",
    "axios": "^1.6.2",
    "form-data": "^4.0.0",
    "i18next": "^23.7.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.72.7",
    "react-native-code-push": "^8.1.0",
    "react-native-config": "^1.5.1",
    "react-native-dropdownalert": "^5.1.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-fbsdk-next": "^12.1.1",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-paper": "^5.11.1",
    "react-native-permissions": "^3.10.1",
    "react-native-push-notification-popup": "^1.7.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^14.0.0",
    "react-native-vector-icons": "^10.0.2",
    "react-navigation-drawer": "react-navigation/drawer",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-actions": "^3.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "uninstall": "^0.0.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/parser": "^7.23.3",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^15.1.0",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.1.0",
    "react-native-rename": "^3.2.13",
    "react-native-svg-transformer": "^1.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16"
  }
}
