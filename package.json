{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env.staging react-native run-android",
    "ios": "ENVFILE=.env.staging react-native run-ios",
    "start": "react-native start",
    "clean:android": "cd android && ./gradlew cleanBuildCache && ./gradlew clean && cd ..",
    "setup:ios": "cd ios && pod install && cd ..",
    "clean:ios": "rm -rf ios/Pods && rm -rf ios/Podfile.lock",
    "clean": "rm -rf node_modules && rm -rf package-lock.json",
    "android:staging": "ENVFILE=.env.staging  react-native run-android --variant=release",
    "android:production": "ENVFILE=.env.production react-native run-android --variant=release",
    "test": "jest",
    "prettier-fix": "prettier --write app/**/*.js",
    "prettier-check": "prettier --list-different **/*.js",
    "lint-fix": "eslint --fix .",
    "lint-check": "eslint .",
    "rename": "react-native-rename"
    
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.9.0",
    "@react-native-community/google-signin": "^4.0.0",
    "@react-native-community/hooks": "^2.5.1",
    "@react-native-community/masked-view": "^0.1.9",
    "@react-native-firebase/analytics": "^7.6.2",
    "@react-native-firebase/app": "^8.4.2",
    "@react-native-firebase/auth": "^9.2.2",
    "@react-native-firebase/crashlytics": "^8.4.4",
    "@react-native-firebase/database": "^7.5.6",
    "@react-native-firebase/firestore": "^7.8.1",
    "@react-native-firebase/messaging": "^7.8.6",
    "@react-native-firebase/storage": "^7.4.3",
    "@react-navigation/bottom-tabs": "^5.2.7",
    "@react-navigation/drawer": "^5.5.0",
    "@react-navigation/native": "^5.1.6",
    "@react-navigation/stack": "^5.2.11",
    "appcenter": "3.0.2",
    "appcenter-analytics": "3.0.2",
    "appcenter-crashes": "3.0.2",
    "axios": "^0.19.2",
    "i18next": "^19.4.3",
    "lodash": "^4.17.19",
    "moment": "^2.29.4",
    "native-base": "^2.13.12",
    "react": "16.13.1",
    "react-i18next": "^11.3.5",
    "react-native": "0.63.2",
    "react-native-app-intro-slider": "^4.0.2",
    "react-native-code-push": "^6.2.0",
    "react-native-config": "^1.3.3",
    "react-native-dropdownalert": "^4.3.0",
    "react-native-fast-image": "^8.1.5",
    "react-native-fbsdk": "^2.0.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-paper": "^3.8.0",
    "react-native-permissions": "^2.2.0",
    "react-native-push-notification-popup": "^1.4.0",
    "react-native-reanimated": "^1.8.0",
    "react-native-restart": "0.0.17",
    "react-native-safe-area-context": "^2.0.0",
    "react-native-screens": "^2.5.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-vector-icons": "6.6.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "uninstall": "0.0.0",
    "yup": "^0.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.9.0",
    "jest": "^26.4.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "prettier": "^2.0.5",
    "react-native-rename": "^2.4.1",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.9.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
